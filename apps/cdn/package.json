{
    "name": "@brawltracker/cdn",
    "packageManager": "yarn@4.0.0-rc.43",
    "version": "1.0.0",
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts",
        "dev": "tsup src/index.ts --format esm,cjs --dts --watch --onSuccess \"node dist/index.js\"",
        "test": "vitest run"
    },
    "devDependencies": {
        "tsup": "^6.7.0",
        "typescript": "5.0.4",
        "vitest": "^0.31.1"
    },
    "type": "module",
    "main": "./dist/index.js",
    "source": "./src/index.ts",
    "dependencies": {
        "@tinyhttp/app": "^2.1.0",
        "@tinyhttp/logger": "^1.3.4",
        "colorette": "^2.0.20"
    }
}
